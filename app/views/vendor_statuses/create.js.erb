$("#vendorStatus").modal('hide');
 $(".status_content").val('');
var url = "<%=all_vendor_status_vendor_statuses_path%>";
$.getJSON(url, function(data) {
	$('#vendor_vendor_status_id').html('<option>Select Vendor status </option>');
	$.each(data, function(index, status) {   
	//alert(batch.name);
	 $('#vendor_vendor_status_id').append($('<option>', { value : status.id }).text(status.status)); 
	});
});
