 <div class="login-screen text-center">
  <%= image_tag "rent.jpg", :class=>"logo", :style=>"width:65px; height:81px;"%>
  <h1 class="tracker-form">RentalTracker</h1>
  <div class="vertical-form sign-in">
   <legend>
      Log In
   </legend>
   
   <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

   <%= f.text_field :email, class: 'is-sensitive',placeholder: "Email Address",label:"false",id:"user_email" %>

   <%= f.password_field :password, class: 'is-sensitive' ,placeholder: "Password",label:"false",id:"user_password"%>

   <%= f.submit 'Login', :class => 'loginbtn animated zoomIn' %>
   <!-- <p><a href="/forgot_password/new">Forgot password?</a></p> -->
 <% end %>
 <br>
 <%= render "devise/shared/links" %>
 </div>
 </div>

