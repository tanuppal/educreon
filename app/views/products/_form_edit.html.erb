<div class="row">
    <div class="col-sm-12">
        <div class="panel with-nav-tabs panel-info">
            <div class="panel-heading">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#productGeneral" data-toggle="tab">General</a></li>
                    <li><a href="#productPricing" data-toggle="tab">Pricing</a></li>
                    <li><a href="#productImage" data-toggle="tab">Image</a></li>
                    <li><a href="#productDescription" data-toggle="tab">Description</a></li>
                    <li><a href="#productAvailiability" data-toggle="tab">Availiablity</a></li>
                    <li><a href="#productBarcode" data-toggle="tab">Inventory</a></li>
                    <li><a href="#productOrders" data-toggle="tab">Orders</a></li>
                    <li><a href="#productBills" data-toggle="tab">Bill</a></li>
                </ul>
            </div>
            <div class="panel-body">
                <%= form_for @general_product, html: {multipart: true} do |t| %>
                <div class="tab-content">
                    <div class="tab-pane fade in active" id="productGeneral">
                        <div class="col-sm-9">
                            <div class="panel panel-info">
                                <div class="panel-heading text-center">General</div>
                                <div class="panel-body products-general-info">
                                    <%= render  partial: 'products/product_general_show', :locals=> {:t=> t} %>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                    <div class="tab-pane fade" id="productPricing">
                        <div class="col-sm-6">
                            <div class="panel panel-info">
                                <div class="panel-heading text-center">Pricing</div>
                                <div class="panel-body product-pricing">
                                    <%= render  partial: 'products/product_pricing_show', :locals=> {:t=> t} %>
                                </div>
                            </div>
                        </div>
                        
                    </div>
                    <div class="tab-pane fade" id="productImage">
                        <div class="col-sm-8">
                            <div class="panel panel-info">
                                <div class="panel-heading text-center">Image</div>
                                <div class="panel-body product-pricing">
                                    <%= render  partial: 'products/product_image', :locals=> {:t=> t} %>
                                </div>
                            </div>
                        </div>
                          <div class="col-sm-7">
                        <%#= render  partial: 'products/product_image_edit' %>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="productDescription">
                        <div class="col-sm-5">
                            <div class="panel panel-info">
                                <div class="panel-heading text-center">Description</div>
                                <div class="panel-body product-pricing">
                                    <%#= render  partial: 'products/product_description', :locals=> {:t=> t} %>
                                </div>
                            </div>
                        </div>
                          <div class="col-sm-7">
                        <%#= render  partial: 'products/product_description_edit' %>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="productBarcode">
                        <div class="col-sm-8">
                            <div class="panel panel-info">
                                <div class="panel-heading text-center">Inventory</div>
                                <div class="panel-body product-pricing">
                                    <%= render  partial: 'products/product_barcode_show', :locals=> {:t=> t} %>
                                </div>
                            </div>
                        </div>
                         <!--  <div class="col-sm-7">
                        <%#= render  partial: 'products/product_barcode_edit' %>
                        </div> -->
                    </div>
                    <div class="tab-pane fade" id="productOrders">
                        <div class="col-sm-5">
                            <div class="panel panel-info">
                                <div class="panel-heading text-center">orders</div>
                                <div class="panel-body products-general-info">
                                    <%= render  partial: 'products/product_orders', :locals=> {:t=> t} %>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-7">
                        <%#= render  partial: 'products/product_orders_edit' %>
                        </div>
                    </div>
                    <div class="tab-pane fade" id="productBills">
                        <div class="col-sm-6">
                            <div class="panel panel-info">
                                <div class="panel-heading text-center">Bills</div>
                                <div class="panel-body products-general-info">
                                    <%= render  partial: 'products/product_bills' %>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- <div class="tab-pane fade" id="vendorOrders">
                        <div class="col-sm-6">
                            <div class="panel panel-info">
                                <div class="panel-heading text-center">Orders</div>
                                <div class="panel-body products-general-info">
                                    <%#= render  partial: 'vendors/vendor_orders' %>
                                </div>
                            </div>
                        </div>
                    </div> -->
                </div>
                <%end%>
            </div>
        </div>
    </div>
</div>

