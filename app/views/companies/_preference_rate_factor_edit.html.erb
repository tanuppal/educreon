<div class="row">
    <!-- Order Tables -->
    <div class="col-md-12 col-xs-12">
        <div class="card mb-3">
            <div class="card-header">
                <i class="fa fa-users vendors-table-users-icon"></i>Rate Factors
                <button class="pull-right btn btn-primary table-print-icon"><i class="fa fa-print"></i></button>
                <button class="pull-right btn btn-primary table-download-icon"><i class="fa fa-download"></i></button>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered table-hover" id="preferenceLogoList" width="100%" cellspacing="0">
                        <thead>
                            <tr class="info">
                                <th>Label</th>
                                <th>Day</th>
                                <th>Factor</th>
                            </tr>
                        </thead>
                            <tfoot>
                                <tr class="info">
                                <th>Label</th>
                                <th>Day</th>
                                <th>Factor</th>
                                </tr>
                            </tfoot>
                        <tbody>
                            <tr>
                                <td>
                                    <%@all_general_company.company_rate_factors.each do |x|%>
                                    <%= x.try(:label) %>
                                    <%end%>  
                                </td>
                                <td>
                                    <%@all_general_company.company_rate_factors.each do |x|%>
                                    <%= x.try(:day) %>
                                    <%end%>   
                                </td>
                                <td>
                                    <%@all_general_company.company_rate_factors.each do |x|%>
                                    <%= x.try(:factor) %>
                                    <%end%>   
                                </td>
                            </tr> 
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
        </div>
    </div>
</div>
<script type="text/javascript">
    // Call the dataTables jQuery plugin
    $(document).ready(function() {
        $('#preferenceLogoList').DataTable({
            "lengthMenu": [[5, 10, 25, 50, -1], [5, 10, 25, 50, "All"]]
        });
    });
</script>