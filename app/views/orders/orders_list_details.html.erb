<div class="content-wrapper">
    <div class="container-fluid">
        <ol class="breadcrumb">
            <li class="breadcrumb-item">
                <a href="index.html">Dashboard</a>
            </li>
            <li class="breadcrumb-item active">Orders</li>
            <!--  <button class="btn btn-primary btn-sm pull-right breadcrumb-button">Add New Product</button> -->
        </ol>
        <div class="row">
            <div class="col-sm-12">
                <div class="panel with-nav-tabs panel-info">
                    <div class="panel-heading">
                        <ul class="nav nav-tabs">
                            <li class="active"><a href="#tab1info" data-toggle="tab">General</a></li>
                            <li><a href="#tab2info" data-toggle="tab">Scheduling</a></li>
                            <li><a href="#billto" data-toggle="tab">Bill To</a></li>
                            <li><a href="#shipto" data-toggle="tab"> Ship to</a></li>
                            <li><a href="#items" data-toggle="tab">Items</a></li>
                            <li><a href="#deliveries" data-toggle="tab">Deliveries</a></li>
                            <li><a href="#invoices" data-toggle="tab">Invoices</a></li>
                            <li><a href="#payments" data-toggle="tab">Payments</a></li>
                            <li><a href="#notes" data-toggle="tab">Notes</a></li>
                            <li><a href="#profit" data-toggle="tab">Profit</a></li>
                        </ul>
                    </div>
                    <div class="panel-body">
                        <div class="tab-content">
                            <div class="tab-pane fade in active" id="tab1info">
                                <div class="col-sm-6">
                                    <div class="panel panel-info">
                                        <div class="panel-heading text-center">General</div>
                                        <div class="panel-body products-general-info">
                                            <%= render  partial: 'orders/order_general' %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade" id="tab2info">
                                <div class="col-sm-6">
                                    <div class="panel panel-info">
                                        <div class="panel-heading text-center">Scheduling</div>
                                        <div class="panel-body product-pricing">
                                            <%= render  partial: 'orders/order_scheduling' %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                              <div class="tab-pane fade" id="billto">
                              <div class="col-sm-6">
                                    <div class="panel panel-info">
                                        <div class="panel-heading text-center">BillTo</div>
                                        <div class="panel-body product-pricing">
                                            <%= render  partial: 'orders/order_bill_to' %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade" id="shipto">
                              <div class="col-sm-6">
                                    <div class="panel panel-info">
                                        <div class="panel-heading text-center">ShipTo</div>
                                        <div class="panel-body product-pricing">
                                            <%= render  partial: 'orders/order_ship_to' %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade" id="items">
                                
                                            <%= render  partial: 'orders/order_items' %>
                                        
                            </div>
                            <div class="tab-pane fade" id="deliveries">
                            
                                <%= render  partial: 'orders/order_deliveries' %>
                                      
                            </div>
                            <div class="tab-pane fade" id="invoices">
                            
                                <%= render  partial: 'orders/order_invoices'%>
                                      
                            </div>
                            <div class="tab-pane fade" id="payments">
                            
                                <%= render  partial: 'orders/order_payments'%>
                             
                                      
                            </div>
                            <div class="tab-pane fade" id="notes">
                             <div class="col-sm-6">
                                    <div class="panel panel-info">
                                        <div class="panel-heading text-center">Notes</div>
                                        <div class="panel-body product-pricing">
                                <%= render  partial: 'orders/order_notes'%>
                            
                             
                                       </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane fade" id="profit">
                            <div class="col-sm-6">
                                    <div class="panel panel-info">
                                        <div class="panel-heading text-center">Profit</div>
                                        <div class="panel-body product-pricing">
                                <%= render  partial: 'orders/order_profit'%>
                                
                            
                             
                                       </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<br>
<br>
<br>

<style type="text/css">
    .panel.with-nav-tabs .panel-heading {
        padding: 5px 5px 0 5px;
    }
    
    .panel.with-nav-tabs .nav-tabs {
        border-bottom: none;
    }
    
    .panel.with-nav-tabs .nav-justified {
        margin-bottom: -1px;
    }
